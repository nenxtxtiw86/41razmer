<Window x:Class="балахнин41.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:балахнин41"
        mc:Ignorable="d"
        Title="OOO 41 Размер" Height="700" Width="1200" FontFamily="Comic Sans MS" Icon="image/icon.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="80"/>
            <RowDefinition/>
            <RowDefinition Height="50"/>
            

        </Grid.RowDefinitions>
        <Image Source="/image/logo.png" VerticalAlignment="Center" HorizontalAlignment="Left"/>
        <TextBlock Text="41 размер" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" ></TextBlock>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Name="BtnBack" Content="Назад" Click="BtnBack_Click" HorizontalAlignment="Right"></Button>
        
    </Grid>
</Window>
